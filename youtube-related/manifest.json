{
  "description": "Move down YouTube's related videos section. See https://github.com/DeimosHall/youtube-related",
  "manifest_version": 3,
  "name": "YouTube Related",
  "version": "0.1.0",
  "homepage_url": "https://github.com/DeimosHall/youtube-related",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "run_at": "document_end",
      "css": [
        "styles.css"
      ],
      "js": [
        "app.js"
      ]
    }
  ]
}